                },
                "lastName": {
                  "type": "string",
                  "example": "Doe",
                  "description": "Last name of the employee"
                },
                "dateBirth": {
                  "type": "string",
                  "example": "1990-01-01",
                  "format": "date",
                  "description": "Date of birth"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@example.com",
                  "format": "email",
                  "description": "Email address"
                },
                "hireDate": {
                  "type": "string",
                  "example": "2024-06-18",
                  "format": "date",
                  "description": "Date of hire"
                },
                "workHours": {
                  "type": "object",
                  "properties": {
                    "start": {
                      "type": "string",
                      "example": "08:00 a.m",
                      "description": "Start time of work"
                    },
                    "end": {
                      "type": "string",
                      "example": "07:00 p.m",
                      "description": "End time of work"
